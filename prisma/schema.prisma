generator client {
  provider      = "prisma-client"
  output        = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

enum Gender {
  MALE
  FEMALE
  NONBINARY
}

model User {
  user_id     Int     @id @default(autoincrement())
  first_name  String
  middle_name String?
  last_name   String
  gender      Gender  @default(MALE)
}
